{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tsqx",
      "camelcase": "Tsqx",
      "title": "Tsqx",
      "scope": "source.tsqx",
      "file-types": [
        "tsqx"
      ],
      "injection-regex": "^tsqx$",
      "class-name": "TreeSitterTsqx"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GPL",
    "description": "A parser for cjquines/tsqx, a wrapper around asymptote.",
    "authors": [
      {
        "name": "extouchtriangle"
      }
    ],
    "links": {
      "repository": "https://github.com/extouchtriangle/tree-sitter-tsqx"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}