{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tsqx",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "function"
            },
            {
              "type": "SYMBOL",
              "name": "set"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "function": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "variable"
          }
        }
      ]
    },
    "command": {
      "type": "PATTERN",
      "value": "rightanglemark|anglemark|circumcenter|orthocenter|incircle|circumcircle|centroid|incenter|midpoint|extension|foot|CP|CR|dir|conj|intersect|IP|OP|Line|bisectorpoint|arc|abs"
    },
    "set": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "equals"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "variable": {
      "type": "PATTERN",
      "value": "([A-Za-z\\&\\'\\_0-9]+)([0-9A-Z\\.]+)?"
    },
    "equals": {
      "type": "PATTERN",
      "value": "(=|;=|:=|\\.=)"
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pair"
        },
        {
          "type": "SYMBOL",
          "name": "function"
        }
      ]
    },
    "pair": {
      "type": "PATTERN",
      "value": "\\([0-9]+\\s*,[0-9]+\\s*\\)"
    },
    "pen": {
      "type": "PATTERN",
      "value": "(pale|light|medium|heavy|dark|deep)?(red|green|blue|cyan|black|white|gray|grey|purple|magenta|pink|yellow|olive|orange|brown)"
    },
    "number": {
      "type": "PATTERN",
      "value": "[\\\\.0-9]\\+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}